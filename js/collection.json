    "strongPassword": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|,.<>\\/?])[A-Za-z0-9!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|,.<>\\/?]{12,}$",
    "alphaNumeric": "^[a-zA-Z0-9]+$",
    "alpha": "^[a-zA-Z]+$",
    "lowercase": "^[a-z]+$",
    "uppercase": "^[A-Z]+$",
    "lettersOnly": "^[A-Za-z]+$",
    "decimal": "^-?\\d*\\.?\\d+$",
    "integer": "^-?\\d+$",
    "positiveInteger": "^\\d+$",
    "negativeInteger": "^-[1-9]\\d*$",
    "float": "^-?\\d*\\.\\d+$",
    "date": "^(0[1-9]|1[0-2])\\/([12][0-9]|3[01])\\/\\d{4}$",
