    "sha1": "^[a-f0-9]{40}$",
    "sha256": "^[a-f0-9]{64}$",
    "base64": "^(?:[A-Z0-9+\\/]{4})*(?:[A-Z0-9+\\/]{2}==|[A-Z0-9+\\/]{3}=)?$",
    "hexadecimal": "^[0-9A-Fa-f]+$",
