    "commaSeparated": "^(?!.*\\,$)([a-zA-Z0-9]+,)*[a-zA-Z0-9]+$",
    "alphaNumericSpaces": "^[a-zA-Z0-9 ]+$",
    "negativeDecimal": "^-\\d*\\.?\\d+$",
    "positiveDecimal": "^\\d*\\.?\\d+$",
    "csvLine": "^([a-zA-Z0-9]+,)*[a-zA-Z0-9]+$",
